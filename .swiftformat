--enable \
    acronyms,\
    blankLinesBetweenImports,\
    blockComments,\
    isEmpty,\
    noExplicitOwnership,\
    organizeDeclarations,\
    redundantProperty,\
    wrapMultilineConditionalAssignment,\

--acronyms "ID,OS,URL,UUID"
--condassignment always
--fractiongrouping enabled
--header "\n {file}\n Genius\n\n Â© {created.year} F1248 <f1248@mailbox.org>\n See LICENSE.txt for license information.\n"
--markcategories false
--maxwidth 128
--organizationmode type
--ranges no-space
--self init-only
--semicolons never
--shortoptionals always
--typeblanklines preserve
--xcodeindentation enabled
